{
  "name": "debug-trace-logits",
  "description": "Trace logits with probes for a single-token decode.",
  "extends": "modes/debug",
  "runtime": {
    "inference": {
      "prompt": "The sky is",
      "batching": { "maxTokens": 1 },
      "sampling": { "temperature": 0 }
    },
    "shared": {
      "debug": {
        "trace": {
          "enabled": true,
          "categories": ["logits"],
          "maxDecodeSteps": 1
        },
        "probes": [
          { "id": "topk", "stage": "logits_final", "tokens": [-1], "dims": [138, 496, 0, 1, 2, 3] }
        ]
      }
    }
  }
}
