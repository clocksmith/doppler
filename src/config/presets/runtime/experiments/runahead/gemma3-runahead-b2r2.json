{
  "extends": "bench",
  "model": "gemma-3-1b-it-wf16",
  "runtime": {
    "shared": {
      "tooling": {
        "intent": "investigate"
      },
      "debug": {
        "logLevel": { "defaultLogLevel": "warn" },
        "trace": { "enabled": false },
        "profiler": { "enabled": false }
      },
      "benchmark": {
        "run": {
          "promptName": "xs",
          "maxNewTokens": 128,
          "warmupRuns": 1,
          "timedRuns": 3,
          "sampling": { "temperature": 0, "topK": 1, "topP": 1 },
          "profile": false,
          "captureMemoryTimeSeries": false
        }
      }
    },
    "inference": {
      "compute": { "activationDtype": "f16" },
      "kvcache": { "kvDtype": "f16" },
      "kernelPath": "gemma3-f16-f16a-online",
      "batching": {
        "batchSize": 2,
        "maxTokens": 128,
        "stopCheckMode": "batch",
        "readbackInterval": 2,
        "ringTokens": 2,
        "ringStop": 2,
        "ringStaging": 2
      },
      "sampling": { "temperature": 0 }
    }
  }
}
