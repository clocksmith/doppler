{
  "extends": "bench",
  "model": "gemma-3-1b-it-wf16",
  "runtime": {
    "shared": {
      "tooling": {
        "intent": "investigate"
      },
      "debug": {
        "logLevel": {
          "defaultLogLevel": "warn"
        },
        "trace": {
          "enabled": false
        },
        "profiler": {
          "enabled": true
        }
      },
      "benchmark": {
        "run": {
          "promptName": "xs",
          "maxNewTokens": 256,
          "warmupRuns": 1,
          "timedRuns": 1,
          "sampling": {
            "temperature": 0,
            "topK": 1,
            "topP": 1
          },
          "profile": true,
          "captureMemoryTimeSeries": false
        }
      }
    },
    "inference": {
      "batching": {
        "batchSize": 1,
        "maxTokens": 256,
        "stopCheckMode": "per-token",
        "readbackInterval": 8,
        "ringTokens": 1,
        "ringStop": 1,
        "ringStaging": 1
      },
      "sampling": {
        "temperature": 0
      },
      "kvcache": {
        "layout": "tiered",
        "kvDtype": "f16",
        "maxSeqLen": 8192,
        "pageSize": 256,
        "windowSize": 1024,
        "tiering": {
          "mode": "int8",
          "hotWindow": 1024,
          "coldPageSize": 256,
          "coldDtype": "f16",
          "compression": {
            "mode": "int8",
            "blockSize": 1
          },
          "gating": {
            "mode": "force_on",
            "minAluBwRatio": 0
          }
        }
      }
    }
  }
}
