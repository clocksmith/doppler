{
  "extends": "bench",
  "model": "gemma-3-1b-it-wq4k",
  "runtime": {
    "shared": {
      "tooling": {
        "intent": "investigate"
      },
      "debug": {
        "logLevel": {
          "defaultLogLevel": "warn"
        },
        "trace": {
          "enabled": false
        },
        "profiler": {
          "enabled": true
        }
      },
      "benchmark": {
        "run": {
          "promptName": "xs",
          "maxNewTokens": 256,
          "warmupRuns": 1,
          "timedRuns": 1,
          "sampling": {
            "temperature": 0,
            "topK": 1,
            "topP": 1
          },
          "profile": true,
          "captureMemoryTimeSeries": false
        }
      }
    },
    "inference": {
      "batching": {
        "batchSize": 1,
        "maxTokens": 256,
        "stopCheckMode": "per-token",
        "readbackInterval": 8,
        "ringTokens": 1,
        "ringStop": 1,
        "ringStaging": 1
      },
      "sampling": {
        "temperature": 0
      }
    }
  }
}
