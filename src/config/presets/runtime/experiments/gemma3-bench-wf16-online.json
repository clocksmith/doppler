{
  "extends": "bench",
  "model": "google-gemma-3-1b-it-wf16",
  "cli": {
    "command": "bench",
    "suite": "inference",
    "profileDir": "tests/results/bench-gemma3-wf16"
  },
  "runtime": {
    "shared": {
      "tooling": { "intent": "investigate" },
      "debug": {
        "logLevel": { "defaultLogLevel": "warn" },
        "trace": { "enabled": false },
        "profiler": { "enabled": true }
      },
      "benchmark": {
        "run": {
          "promptName": "xs",
          "maxNewTokens": 256,
          "warmupRuns": 1,
          "timedRuns": 1,
          "sampling": { "temperature": 0, "topK": 1, "topP": 1 },
          "profile": true,
          "captureMemoryTimeSeries": false
        }
      }
    },
    "inference": {
      "kernelPath": "gemma3-f16-f16a-online",
      "batching": {
        "batchSize": 1,
        "maxTokens": 256,
        "stopCheckMode": "per-token",
        "readbackInterval": 8,
        "ringTokens": 1,
        "ringStop": 1,
        "ringStaging": 1
      },
      "sampling": { "temperature": 0 },
      "kvcache": {
        "layout": "contiguous",
        "windowSize": 256
      }
    }
  }
}
