{
  "extends": "bench",
  "model": "google-gemma-3-1b-it-wq4k-ef16",
  "runtime": {
    "shared": {
      "tooling": {
        "intent": "investigate"
      },
      "debug": {
        "logLevel": {
          "defaultLogLevel": "warn"
        },
        "trace": {
          "enabled": false
        }
      },
      "benchmark": {
        "run": {
          "promptName": "medium",
          "maxNewTokens": 64,
          "warmupRuns": 1,
          "timedRuns": 1,
          "sampling": {
            "temperature": 0,
            "topK": 1,
            "topP": 1
          },
          "profile": false,
          "captureMemoryTimeSeries": false
        }
      }
    },
    "inference": {
      "batching": {
        "batchSize": 4,
        "maxTokens": 64,
        "stopCheckMode": "per-token",
        "readbackInterval": 8,
        "ringTokens": 2,
        "ringStop": 2,
        "ringStaging": 2
      },
      "sampling": {
        "temperature": 0
      }
    }
  }
}
