{
  "extends": "bench",
  "model": "google-gemma-3-1b-it-wq4k-ef16",
  "cli": {
    "command": "bench",
    "suite": "inference"
  },
  "runtime": {
    "shared": {
      "tooling": {
        "intent": "investigate"
      },
      "debug": {
        "logLevel": {
          "defaultLogLevel": "warn"
        },
        "trace": {
          "enabled": false
        },
        "profiler": {
          "enabled": true
        }
      },
      "benchmark": {
        "run": {
          "profile": true,
          "captureMemoryTimeSeries": true,
          "memoryTimeSeriesIntervalMs": 50,
          "warmupRuns": 1,
          "timedRuns": 3
        }
      }
    },
    "inference": {
      "prompt": "The color of the sky is",
      "batching": {
        "batchSize": 4,
        "maxTokens": 128,
        "stopCheckMode": "per-token",
        "readbackInterval": 4,
        "ringTokens": 1,
        "ringStop": 1,
        "ringStaging": 1
      },
      "sampling": { "temperature": 0 }
    }
  }
}
