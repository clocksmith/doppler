{
  "extends": "bench",
  "model": "gemma-3-1b-it-wf16",
  "runtime": {
    "shared": {
      "tooling": {
        "intent": "investigate"
      },
      "debug": {
        "logLevel": { "defaultLogLevel": "warn" },
        "trace": { "enabled": false },
        "profiler": {
          "enabled": true,
          "logEveryDecodeSteps": 1
        }
      },
      "benchmark": {
        "run": {
          "promptName": "xs",
          "maxNewTokens": 64,
          "warmupRuns": 1,
          "timedRuns": 1,
          "sampling": { "temperature": 0, "topK": 1, "topP": 1 },
          "profile": true,
          "captureMemoryTimeSeries": false
        }
      }
    },
    "inference": {
      "compute": { "activationDtype": "f16" },
      "kvcache": { "kvDtype": "f16" },
      "kernelPath": "gemma3-f16-f16a-online",
      "batching": {
        "batchSize": 4,
        "maxTokens": 64,
        "stopCheckMode": "batch",
        "readbackInterval": 4,
        "ringTokens": 4,
        "ringStop": 4,
        "ringStaging": 4
      },
      "sampling": { "temperature": 0 }
    }
  }
}
