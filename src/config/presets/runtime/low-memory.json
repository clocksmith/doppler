{
  "name": "low-memory",
  "description": "Low memory configuration for constrained environments",
  "extends": "default",
  "runtime": {
    "shared": {
      "debug": {
        "logHistory": {
          "maxLogHistoryEntries": 100
        }
      }
    },
    "inference": {
      "kvcache": {
        "maxSeqLen": 2048,
        "kvDtype": "f16",
        "pageSize": 128
      },
      "moe": {
        "cache": {
          "dequantCacheMaxEntries": 2
        }
      },
      "batching": {
        "maxTokens": 512
      }
    }
  }
}
