{
  "extends": "bench",
  "model": "google-gemma-3-1b-it-wq4k-ef16",
  "cli": {
    "command": "bench",
    "suite": "inference"
  },
  "runtime": {
    "shared": {
      "tooling": {
        "intent": "investigate"
      },
      "debug": {
        "logLevel": { "defaultLogLevel": "warn" },
        "trace": { "enabled": false },
        "profiler": { "enabled": true }
      },
      "benchmark": {
        "run": {
          "promptName": "short",
          "maxNewTokens": 8,
          "warmupRuns": 1,
          "timedRuns": 1,
          "sampling": { "temperature": 0, "topK": 1, "topP": 1 },
          "profile": true,
          "captureMemoryTimeSeries": false
        }
      }
    },
    "inference": {
      "batching": {
        "batchSize": 4,
        "maxTokens": 8,
        "stopCheckMode": "per-token",
        "readbackInterval": 4,
        "ringTokens": 1,
        "ringStop": 1,
        "ringStaging": 1
      },
      "sampling": { "temperature": 0 }
    }
  }
}
